{
  "name": "tedium",
  "version": "1.0.0",
  "description": "A bot for doing mass changes to github repos for the Polymer team",
  "main": "tedium.js",
  "bin": "tedium.js",
  "scripts": {
    "test": "tslint *.ts cleanup-passes/*.ts custom_typings/*.ts",
    "build": "tsc",
    "build:watch": "tsc -w",
    "test:watch": "watch 'npm test' ./",
    "prepublish": "npm run build",
    "postinstall": "tsd install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PolymerLabs/tedium.git"
  },
  "keywords": [
    "bot"
  ],
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/PolymerLabs/tedium/issues"
  },
  "homepage": "https://github.com/PolymerLabs/tedium#readme",
  "dependencies": {
    "command-line-args": "^2.0.4",
    "dom5": "^1.3.1",
    "escodegen": "^1.7.0",
    "espree": "^2.2.5",
    "estree-walker": "^0.2.0",
    "github": "^0.2.0",
    "github-cache": "^1.1.0",
    "hydrolysis": "^1.19.3",
    "js-yaml": "^3.4.3",
    "nodegit": "^0.7.0",
    "pad": "^1.0.0",
    "progress": "^1.1.8",
    "promisify-node": "^0.3.0",
    "rimraf": "^2.4.3",
    "strip-json-comments": "^2.0.0"
  },
  "devDependencies": {
    "tsd": "^0.6.5",
    "tslint": "^3.1.1",
    "typescript": "^1.7.3",
    "watch": "^0.17.1"
  }
}
